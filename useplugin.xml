<?xml version="1.0" encoding="UTF-8"?>
<?use version="6.0.0"?>
<plugin name="STMPlugin" version="1.0">
    <actions>
        <action label="STM Transformation"
                icon="resources/idk.png"
                class="org.tzi.use.STMPlugin.STMPluginAction"
                tooltip="Input a UML class model and file with TOCL properties and transform."
                menu="STM Transformation"
                menuitem="STM Transformation"
                toolbaritem="plugins"
                id="org.tzi.use.STMPlugin.STMPluginAction">
        </action>
    </actions>
    <commands>
        <command shellcmd="stmTransform"
                label="STMTransformation"
                help="Transforms current model into a Snapshot Transition Model and translates TOCL properties to OCL."
                class="org.tzi.use.STMPlugin.STMPluginCMD"
                id="org.tzi.use.STMPlugin.STMPluginCMD">
        </command>
    </commands>
</plugin>